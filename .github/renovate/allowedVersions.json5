{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["helm"],
      "matchPackagePatterns": ["ingress-nginx"],
      "allowedVersions": "<=4.10.0"
    },
    {
      "matchManagers": ["flux"],
      "matchDatasources": ["git-refs", "git-tags", "github-releases", "github-runners", "github-tags", "helm", "docker"],
      "matchPackageNames": ["clusters/flux-system/gotk-components.yaml"],
      "versioning": {
        "pin": {
          "version": "2.2.0"
        }
      }
    }
  ]
}
